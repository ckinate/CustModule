												}


											</div>
											<!--end: Wizard Step 6-->
											<!--begin: Wizard Step 7-->
											<div class="pb-5" data-wizard-type="step-content">
												<h4 class="mb-10 font-weight-bold text-dark">Documents</h4>
												@if (PageModel.Documents != null && PageModel.Documents.Documents.Count > 0)
												{
													foreach (var (document, index) in PageModel.Documents.Documents.Select((v, i) => (v, i)))
													{
													var issueDate = document.IssueDate.HasValue ? document.IssueDate.Value.ToShortDateString() : "";
													var expiryDate = document.ExpiryDate.HasValue ? document.ExpiryDate.Value.ToShortDateString() : "";

														<input type="hidden" @bind-value="@document.DocumentId" />
														<div>
															<div class="row">
																<div class="col-xl-12">
																	<!--begin::Input-->
																	<div class="form-group">
																		<InputText class="form-control form-control-solid form-control-lg" @bind-Value="document.Title" readonly></InputText>
																	</div>
																	<!--end::Input-->
																</div>
																
															</div>

														<div class="row">
															
															<div class="col-xl-8">
																<!--begin::Group-->
																<div class="form-group">

																	<div class="col-form-label">
																		<div class="checkbox-inline">

																			<label class="checkbox">
																				<InputCheckbox @bind-Value="document.HasIssueDate" readonly></InputCheckbox>
																				<span></span>Has issue date
																			</label>


																			<label class="checkbox">
																				<InputCheckbox @bind-Value="document.HasExpiryDate" readonly></InputCheckbox>
																				<span></span>Has expiry date
																			</label>
																		</div>
																	</div>
																</div>
																<!--end::Group-->
															</div>
														</div>

														@if (PageModel.Documents.Documents[index].HasIssueDate && PageModel.Documents.Documents[index].HasExpiryDate)
														{
															<div class="row">
																<div class="col-xl-6">
																	<!--begin::Input-->
																	<div class="form-group">
																		<InputText class="form-control form-control-solid form-control-lg" @bind-Value="issueDate" readonly></InputText>
																	</div>
																	<!--end::Input-->
																</div>
																<div class="col-xl-6">
																	@if (document.HasExpiryDate)
																	{
																		<!--begin::Input-->
																		<div class="form-group">
																			<InputText class="form-control form-control-solid form-control-lg" @bind-Value="expiryDate" readonly></InputText>
																		</div>
																		<!--end::Input-->
																	}

																</div>
															</div>
														}
														else if (PageModel.Documents.Documents[index].HasIssueDate)
														{
															<div class="row">
																<div class="col-xl-6">
																	<!--begin::Input-->
																	<div class="form-group">
																		<InputText class="form-control form-control-solid form-control-lg" @bind-Value="issueDate" readonly></InputText>
																	</div>
																	<!--end::Input-->
																</div>
																
															</div>
														}
														else if (PageModel.Documents.Documents[index].HasExpiryDate)
														{
															<div class="row">
																
																<div class="col-xl-6">
																	@if (document.HasExpiryDate)
																	{
																		<!--begin::Input-->
																		<div class="form-group">
																			<InputText class="form-control form-control-solid form-control-lg" @bind-Value="expiryDate" readonly></InputText>
																		</div>
																		<!--end::Input-->
																	}

																</div>
															</div>
														}

															<!--begin::Input-->
															<div class="form-group">
															<a href="@document.LocationUrl" target="_blank">Click to view document</a>
															</div>
															<!--end::Input-->
														</div>
													}
												}

												<!--begin::Form group-->
												<div class="form-group">
													
												</div>
												<!--end::Form group-->
											</div>
											<!--end: Wizard Step 7-->
										<!--begin: Wizard Step 8-->
										<div class="pb-5" data-wizard-type="step-content">
											<h4 class="mb-10 font-weight-bold text-dark">Additional Informations</h4>
											@if (PageModel.CustomFields != null && PageModel.CustomFields.CustomFields.Count > 0)
											{
												@foreach (var (field, index) in PageModel.CustomFields.CustomFields.Select((v, i) => (v, i)))
												{

													<div class="row">
														<input type="hidden" @bind-value="@field.FieldId" />

														<div class="col-xl-12">
															<!--begin::Input-->
															<div class="form-group">
																<label>
																	@field.Field
																	@if (field.Compulsory)
																	{
																		<span class="text-danger">*</span>
																	}
																</label>

																<InputText class="form-control form-control-lg form-control-solid" @bind-Value="PageModel.CustomFields.CustomFields[index].Response" readonly></InputText>
																
															</div>
															<!--end::Input-->
														</div>
													</div>

												}
											}


										</div>
										<!--end: Wizard Step 8-->


							
											<!--begin: Wizard Actions-->
											<div class="d-flex justify-content-between border-top mt-5 pt-10">
												<div class="mr-2">
													<button type="button" class="btn btn-light-primary font-weight-bolder text-uppercase px-9 py-4" data-wizard-type="action-prev">Previous</button>
												</div>
												<div>
													@*<button type="button" class="btn btn-success font-weight-bolder text-uppercase px-9 py-4" data-wizard-type="action-submit">Submit</button>*@

												<button type="button" class="btn btn-primary font-weight-bolder text-uppercase px-9 py-4" data-wizard-type="action-next">Next</button>
												
												</div>
											</div>
											<!--end: Wizard Actions-->

									}

									@*</form>*@
								</div>
								<!--end: Wizard-->
							</div>
							<!--end: Wizard Form-->
						</div>
						<!--end: Wizard Body-->
					</div>
					<!--end: Wizard-->
				</div>
			</div>
		</div>
		<!--end::Container-->
	</div>
	<!--end::Entry-->
</div>
<!--end::Content-->