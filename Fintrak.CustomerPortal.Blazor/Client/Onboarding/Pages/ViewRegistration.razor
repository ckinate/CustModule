					<!--begin::Page Title-->
					<h5 class="text-dark font-weight-bold my-1 mr-5">Registration</h5>
					<!--end::Page Title-->
					<!--begin::Breadcrumb-->
					<ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
						<li class="breadcrumb-item text-muted">
							<a href="#" class="text-muted">Onboarding</a>
						</li>
						<li class="breadcrumb-item text-muted">
							<a href="#" class="text-muted">Registration</a>
						</li>
					</ul>
					<!--end::Breadcrumb-->
				</div>
				<!--end::Page Heading-->
			</div>
			<!--end::Info-->
			<!--begin::Toolbar-->

			<div class="d-flex align-items-center">
				@*@if(PageModel.OfficialInformation.Status == OnboardingStatus.Processing)
				{
					<a href="/onboarding/registration" class="btn btn-light-primary font-weight-bolder btn-sm">Update Information</a>
				}*@

				@if (PageModel.CanUpdate)
				{
					<a href="/onboarding/registration" class="btn btn-light-primary font-weight-bolder btn-sm">Update Information</a>
				}
				
			</div>
			<!--end::Toolbar-->
		</div>
	</div>
	<!--end::Subheader-->
