						</span>
						<h3 class="card-label">
							Customer Onboarding
							<small>

								@if(PageModel != null){
									if (PageModel.OfficialInformation.Status == OnboardingStatus.Submitted)
									{
										<span class="label label-warning label-pill label-inline mr-2">Submitted</span>
									}
									else if (PageModel.OfficialInformation.Status == OnboardingStatus.Processing)
									{
										<span class="label label-info label-inline mr-2">Processing</span>
									}
									else if (PageModel.OfficialInformation.Status == OnboardingStatus.Queried)
									{
										<span class="label label-danger label-pill label-inline mr-2">Enqueried</span>
									}
									else if (PageModel.OfficialInformation.Status == OnboardingStatus.Completed)
									{
										<span class="label label-dark label-inline mr-2">Completed</span>
									}
									else
									{
										<span class="label label-danger label-pill label-inline mr-2">New</span>
									}

									@if (!PageModel.OfficialInformation.DueDiligenceCompleted)
									{
										<span class="label label-danger label-pill label-inline mr-2">Due diligence in progress</span>
									}
								}
								else
								{
									<span class="label label-danger label-pill label-inline mr-2">New</span>
								}
								
							</small>
						</h3>
					</div>
