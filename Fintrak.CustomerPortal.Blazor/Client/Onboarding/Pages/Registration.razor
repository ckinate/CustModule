																		<div class="col-form-label">
																			<div class="checkbox-inline">
																				<label class="checkbox">
																					<InputCheckbox @bind-Value="PageModel.Documents.Documents[index].HasIssueDate"></InputCheckbox>
																					<span></span>has issue date
																				</label>
																			</div>
																		</div>
																	</div>
																	<!--end::Input-->
																</div>
																<div class="col-xl-6">
																	<!--begin::Input-->
																	<div class="form-group">
																		<label class=""></label>
																		<div class="col-form-label">
																			<div class="checkbox-inline">
																				<label class="checkbox">
																					<InputCheckbox @bind-Value="PageModel.Documents.Documents[index].HasExpiryDate"></InputCheckbox>
																					<span></span>has expiry date
																				</label>
																			</div>
																		</div>
																	</div>
																	<!--end::Input-->
																</div>
															</div>

															@if (PageModel.Documents.Documents[index].HasIssueDate && PageModel.Documents.Documents[index].HasExpiryDate)
															{
																<div class="row">

																	<div class="col-xl-6">
																		<!--begin::Input-->
																		<div class="form-group">
																			<label>Issue Date<span class="text-danger">*</span></label>

																			<CustomInputDateNullable class="form-control form-control-solid form-control-lg" ValueExpression="@(()=> PageModel.Documents.Documents[index].IssueDate)" Value="PageModel.Documents.Documents[index].IssueDate" ValueChanged="@((DateTime? value) => OnIssueDateValueChanged(value, document.FormId))"></CustomInputDateNullable>

																			<ValidationMessage For="@(() => PageModel.Documents.Documents[index].IssueDate)" class="form-text text-danger" />
																		</div>
																		<!--end::Input-->
																	</div>
																	<div class="col-xl-6">
																		<!--begin::Input-->
																		<div class="form-group">
																			<label>Expiry Date<span class="text-danger">*</span></label>

																			<CustomInputDateNullable class="form-control form-control-solid form-control-lg" ValueExpression="@(()=> PageModel.Documents.Documents[index].ExpiryDate)" Value="PageModel.Documents.Documents[index].ExpiryDate" ValueChanged="@((DateTime? value) => OnExpiryDateValueChanged(value, document.FormId))"></CustomInputDateNullable>

																			<ValidationMessage For="@(() => PageModel.Documents.Documents[index].ExpiryDate)" class="form-text text-danger" />
																		</div>
																		<!--end::Input-->
																	</div>
																</div>
															}
															else if (PageModel.Documents.Documents[index].HasIssueDate)
															{
																<div class="row">
																	<div class="col-xl-6">
																		<!--begin::Input-->
																		<div class="form-group">
																			<label>Issue Date<span class="text-danger">*</span></label>

																			<CustomInputDateNullable class="form-control form-control-solid form-control-lg" ValueExpression="@(()=> PageModel.Documents.Documents[index].IssueDate)" Value="PageModel.Documents.Documents[index].IssueDate" ValueChanged="@((DateTime? value) => OnIssueDateValueChanged(value, document.FormId))"></CustomInputDateNullable>

																			<ValidationMessage For="@(() => PageModel.Documents.Documents[index].IssueDate)" class="form-text text-danger" />
																		</div>
																		<!--end::Input-->
																	</div>
																	<div class="col-xl-6">
																	</div>
																</div>
															}
															else if (PageModel.Documents.Documents[index].HasExpiryDate)
															{
																<div class="row">

																	<div class="col-xl-6">
																		<!--begin::Input-->
																		<div class="form-group">
																			<label>Expiry Date<span class="text-danger">*</span></label>

																			<CustomInputDateNullable class="form-control form-control-solid form-control-lg" ValueExpression="@(()=> PageModel.Documents.Documents[index].ExpiryDate)" Value="PageModel.Documents.Documents[index].ExpiryDate" ValueChanged="@((DateTime? value) => OnExpiryDateValueChanged(value, document.FormId))"></CustomInputDateNullable>

																			<ValidationMessage For="@(() => PageModel.Documents.Documents[index].ExpiryDate)" class="form-text text-danger" />
																		</div>
																		<!--end::Input-->
																	</div>
																</div>
															}


															<div class="row">
																<!--begin::Input-->
																<div class="form-group">
																	<label>File<span class="text-danger">*</span></label>

																	<InputFile OnChange="@((InputFileChangeEventArgs e) => HandleFileSelection(e, document.FormId))" class="form-control form-control-solid form-control-lg" accept=".png,.jpeg,.jpg,.doc,.docx,.csv,.xlsx,.xls" />

																	@if (!string.IsNullOrEmpty(document.SelectedFileName))
																	{
																		<label>Selected file: @document.SelectedFileName </label>
																	}

																	<ValidationMessage For="@(() => PageModel.Documents.Documents[index].FileData)" class="form-text text-muted" />

																</div>
																<!--end::Input-->
															</div>
															

															<div class="row">
																@if (!string.IsNullOrEmpty(document.LocationUrl))
																{
																	<!--begin::Input-->
																	<div class="form-group">
																		<a href="@document.LocationUrl" target="_blank">Click to view document</a>

																	</div>
																	<!--end::Input-->
																}
															</div>
															

														</div>
													}
												}

												<!--begin::Form group-->
												<div class="form-group">
													<a href="javascript:;" class="btn btn-light-primary" @onclick="AddDocument">
														<i class="la la-plus"></i>Add
													</a>
												</div>
												<!--end::Form group-->
											</div>
											<!--end: Wizard Step 7-->
											<!--begin: Wizard Step 8-->
											<div class="pb-5" data-wizard-type="step-content">
												<h4 class="mb-10 font-weight-bold text-dark">Additional Informations</h4>
												@if (PageModel.CustomFields != null && PageModel.CustomFields.CustomFields.Count > 0)
												{

													@foreach (var (field, index) in PageModel.CustomFields.CustomFields.Select((v, i) => (v, i)))
													{

														<div class="row">
															<input type="hidden" @bind-value="@field.FieldId" />
															
															<div class="col-xl-12">
																<!--begin::Input-->
																<div class="form-group">
																	<label>@field.Field
																		@if (field.Compulsory)
																		{
																			<span class="text-danger">*</span>
																		}
																	</label>

																	<InputText class="form-control form-control-lg form-control-solid" @bind-Value="PageModel.CustomFields.CustomFields[index].Response"></InputText>
																	<ValidationMessage For="@(() => PageModel.CustomFields.CustomFields[index].Response)" class="form-text text-danger" />
																</div>
																<!--end::Input-->
															</div>
														</div>
									
													}
												}


											</div>
											<!--end: Wizard Step 8-->

											<!--begin: Wizard Step 8-->
											<div class="pb-5" data-wizard-type="step-content">
												<!--begin::Wrapper-->
												<div class="w-100">
													<!--begin::Heading-->
													<div class="pb-8 pb-lg-10">
														<!--begin::Title-->
														<h2 class="fw-bolder text-dark">Review and Submit</h2>
														<!--end::Title-->
													</div>
													<!--end::Heading-->
													<!--begin::Body-->
													<button type="button" class="btn btn-primary btn-lg btn-block" @onclick="ShowReview">Click to review</button>
													<!--end::Body-->
												</div>
												<!--end::Wrapper-->

											</div>
											<!--end: Wizard Step 8-->
											<!--begin: Wizard Actions-->
											<div class="d-flex justify-content-between border-top mt-5 pt-10">
												<div class="mr-2">
													@*<button type="submit">Save</button>*@
													<button type="button" class="btn btn-light-primary font-weight-bolder text-uppercase px-9 py-4" data-wizard-type="action-prev">Previous</button>
												</div>
												<div>
													<button type="button" class="btn btn-success font-weight-bolder text-uppercase px-9 py-4" data-wizard-type="action-submit">Submit</button>


													<button type="button" class="btn btn-primary font-weight-bolder text-uppercase px-9 py-4" data-wizard-type="action-next">Next</button>
												</div>
											</div>
											<!--end: Wizard Actions-->
									
									@*</form>*@
								</div>
								<!--end: Wizard-->
							</div>
							<!--end: Wizard Form-->
						</div>
						<!--end: Wizard Body-->
					</div>
					<!--end: Wizard-->
						</EditForm>
					}


					
				</div>
			</div>
		</div>
		<!--end::Container-->
	</div>
	<!--end::Entry-->
</div>
<!--end::Content-->

<ReviewDialog @ref="ReviewDialog"></ReviewDialog>
